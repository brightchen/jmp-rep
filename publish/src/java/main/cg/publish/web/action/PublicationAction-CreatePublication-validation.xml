<!DOCTYPE validators PUBLIC "-//OpenSymphony Group//XWork Validator 1.0.2//EN"
    "http://www.opensymphony.com/xwork/xwork-validator-1.0.2.dtd">
<validators>
	
    <field name="publication.publicationname">
        <field-validator type="requiredstring">
            <param name="trim">true</param>
            <message key="errors.required"/>
        </field-validator>
        <field-validator type="regex" short-circuit="false">
 	    <param name="expression">^[a-zA-Z0-9_]+$</param> 
  	    <message key="errors.no.space.allow" /> 
  	</field-validator>
    </field>
	<field name="publication.publicationfullname">
        <field-validator type="requiredstring">
            <param name="trim">true</param>
            <message key="errors.required"/>
        </field-validator>
	</field>
    <field name="publication.keyword">
        <field-validator type="requiredstring">
            <param name="trim">true</param>
            <message key="errors.required"/>
        </field-validator>
    </field>
    <!-- 
   	<field name="publication.startdate">
        <field-validator type="required">
            <message key="errors.required"/>
        </field-validator>
      
    </field> 
    <field name="publication.enddate">
        <field-validator type="required">
            <message key="errors.required"/>
        </field-validator>     
    </field>
 	-->
</validators>