<validators>	
   
    <field name="publication.publicationfullname">
        <field-validator type="requiredstring">
            <param name="trim">true</param>
            <message key="errors.required"/>
        </field-validator>
    </field>
    <field name="publication.keyword">
        <field-validator type="requiredstring">
            <param name="trim">true</param>
            <message key="errors.required"/>
        </field-validator>
    </field>
